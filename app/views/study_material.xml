<Alloy>
	<Window id="win" navBarHidden="true" backgroundColor="black">
		<View id="vertical" backgroundColor="white">
			<NavigationGroup id="navgroup">
           <Window navBarHidden="true">
			<View id="header">
				<Label id="title">Study Material</Label>
				<Require id="settings_back_btn" type="view" src="store_back_btn" onClick="closeModal"/>
			</View>
			<ScrollView id="scrollView" showVerticalScrollIndicator="true">
				<Label id="page_title">Helpful resources from CSA Group</Label>
				<TableView id="tv" style="Ti.UI.iPhone.TableViewStyle.GROUPED" backgroundColor="transparent" scrollable="false" onClick="handleTableViewClick">
					<TableViewRow id="tablerowheight" touchEnabled="true" backgroundColor="white" hasChild="true" selectionStyle="Ti.UI.iPhone.TableViewCellSelectionStyle.GRAY">
						<View left="10" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
							<View layout="vertical" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
								<Label class="title">Study Guides</Label>
								<Label class="subtitle">Hard copy guides to help you prepare for your C of Q</Label>
							</View>
						</View>
					</TableViewRow>
					<TableViewRow id="tablerowheight" touchEnabled="true" backgroundColor="white" hasChild="true" selectionStyle="Ti.UI.iPhone.TableViewCellSelectionStyle.GRAY">
						<View left="10" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
							<View layout="vertical" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
								<Label class="title">Electrical Quick Reference</Label>
								<Label class="subtitle">Commonly used terms, rules, installation practices and calculations</Label>
							</View>
						</View>
					</TableViewRow>
					<TableViewRow id="tablerowheight" touchEnabled="true" backgroundColor="white" hasChild="true" selectionStyle="Ti.UI.iPhone.TableViewCellSelectionStyle.GRAY">
						<View left="10" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
							<View layout="vertical" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
								<Label class="title">Electrical Code</Label>
								<Label class="subtitle">Canadian electrical code, part I (22nd edition), safety standard for electrical installations</Label>
							</View>
						</View>
					</TableViewRow>
					<TableViewRow id="tablerowheight" touchEnabled="true" backgroundColor="white" hasChild="true" selectionStyle="Ti.UI.iPhone.TableViewCellSelectionStyle.GRAY">
						<View left="10" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
							<View layout="vertical" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
								<Label class="title">CE Code Handbook</Label>
								<Label class="subtitle">CE code handbook, an explanation of rules of the canadian electrical code, part 1</Label>
							</View>
						</View>
					</TableViewRow>
				</TableView>
			</ScrollView>
			</Window>
</NavigationGroup>
		</View>
	</Window>
</Alloy>
<Alloy>
        <Window id="modal" navBarHidden="true">
                <View id="vertical">
                        <View id="header">
                                <Label id="title">Question Set</Label>
                                <Require id="settings_back_btn" type="view" src="settings_back_btn" onClick="closeModal"/>
                        </View>
                        <ScrollView id="scrollView" showVerticalScrollIndicator="true">

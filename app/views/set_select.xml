                                                        <View layout="vertical" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
                                                                <Label class="title">Block B</Label>
                                                        </View>
                                                </View>
                                        </TableViewRow>
                                        <TableViewRow class="tableviewrowheight2" touchEnabled="false" backgroundColor="white" selectionStyle="Ti.UI.iPhone.TableViewCellSelectionStyle.GRAY">
                                                <View left="10" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
                                                        <View layout="vertical" width="Ti.UI.SIZE" height="Ti.UI.SIZE">
                                                                <Label class="title">Block C</Label>
                                                        </View>
                                                </View>
                                        </TableViewRow>
                                        <TableViewRow class="tableviewrowheight2" touchEnabled="false" backgroundColor="white" selectionStyle="Ti.UI.iPhone.TableViewCellSelectionStyle.GRAY">

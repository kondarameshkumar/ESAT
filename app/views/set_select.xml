                        <Label id="page_title">Question Sets</Label>
                        <TableView id="tv"  style="Ti.UI.iPhone.TableViewStyle.GROUPED" backgroundColor="transparent" scrollable="false" onClick="handleTableViewClick">
                                <TableViewSection>
                                        <TableViewRow class="tableviewrowheight1"   touchEnabled="false" backgroundColor="white" selectionStyle="Ti.UI.iPhone.TableViewCellSelectionStyle.GRAY">
